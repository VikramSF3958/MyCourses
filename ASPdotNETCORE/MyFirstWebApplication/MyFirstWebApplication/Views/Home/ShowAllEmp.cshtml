@model IEnumerable<MyFirstWebApplication.Models.Employee>

@{
    ViewBag.Title = "Showing All Employees";
}

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.id</td>
            <td>@employee.Name</td>
        </tr>
    }
</table>
